class Solution {
public:
    vector<int> topKFrequent(vector<int>& num, int k) {
        unordered_map<int,int>mp;
        for(int i=0;i<num.size();i++){
            mp[num[i]]++;
        }
        priority_queue<pair<int,int>>pq;
        for(auto e:mp){
            pq.push({e.second,e.first});
        }
        vector<int>v;
        while(k){
            v.push_back(pq.top().second);
            pq.pop();k--;
        }
        return v;
    }
};
